{
	"name": "read-json",
	"displayName": "Read JSON value",
	"description": "Read JSON Value (statically or dynamically)",
	"version": "1.0.1",
	"publisher": "FaustVX",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/FaustVX/vscode-read-json"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:read-json.read"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "read-json.read",
				"title": "Read JSON value"
			}
		],
		"menus": {
			"explorer/context": [
			{
			  "command": "read-json.read",
			  "group": "2_workspace"
			}
		   ]
		   }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"vsce": "^1.75.0",
		"typescript": "^5.2.2"
	}
}
